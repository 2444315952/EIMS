            </if>
        </set>
        where OUTBOUND_DETAIL_ID = #{outboundDetailId}
    </update>

    <!--批量修改数据-->
    <update id="updateBatch">
        <foreach collection="list" item="entity" open="begin" separator=";" close=";end;">
            update EIMS.OUTBOUND_DETAIL
            <set>
                <if test="entity.stockOutId != null">
