            </if>
            <if test="deliveryQuantity != null">
                #{deliveryQuantity},
            </if>
            <if test="commodityNote != null">
                #{commodityNote},
            </if>
        </trim>
        <selectKey keyProperty="outboundDetailId" resultType="INTEGER" order="BEFORE">
            select SEQ_OUTBOUND_DETAIL.nextval from dual
        </selectKey>
    </insert>

