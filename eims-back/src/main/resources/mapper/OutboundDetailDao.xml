                    #{entity.commodityNote},
                </if>
            </trim>
        </foreach>
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update EIMS.OUTBOUND_DETAIL
        <set>
            <if test="stockOutId != null">
                STOCK_OUT_ID = #{stockOutId},
            </if>
            <if test="stockOutDocunum != null">
                STOCK_OUT_DOCUNUM = #{stockOutDocunum},
            </if>
            <if test="productId != null">
