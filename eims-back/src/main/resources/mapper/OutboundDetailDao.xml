            </if>
            <if test="deliveryQuantity != null">
                DELIVERY_QUANTITY,
            </if>
            <if test="commodityNote != null">
                COMMODITY_NOTE,
            </if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            #{outboundDetailId},
            <if test="stockOutId != null">
                #{stockOutId},
            </if>
            <if test="stockOutDocunum != null">
                #{stockOutDocunum},
            </if>
            <if test="productId != null">
                #{productId},
            </if>
            <if test="productPicture != null">
