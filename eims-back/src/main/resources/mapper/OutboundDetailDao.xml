        delete from EIMS.OUTBOUND_DETAIL where OUTBOUND_DETAIL_ID in
        <foreach collection="list" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <!--通过实体列表批量删除数据-->
