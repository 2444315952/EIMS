        from (select <include refid="PurchaseReturnCol"></include> from EIMS.PURCHASE_RETURN
        <where>
            company_id = #{companyId} and is_out = 0
            <if test="workPointId != null">and work_point_id = #{workPointId}</if>
        </where>
        )

        <where>
            <if test="purchReturnId != null">
