<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eims.mybatis.dao.SupplierDao">

    <resultMap type="com.eims.mybatis.entity.Supplier" id="SupplierMap">
        <result property="supplierId" column="SUPPLIER_ID" jdbcType="INTEGER"/>
        <result property="supplierName" column="SUPPLIER_NAME" jdbcType="VARCHAR"/>
        <result property="supplierType" column="SUPPLIER_TYPE" jdbcType="VARCHAR"/>
        <result property="contact" column="CONTACT" jdbcType="VARCHAR"/>
        <result property="contactNumber" column="CONTACT_NUMBER" jdbcType="VARCHAR"/>
        <result property="contactAddress" column="CONTACT_ADDRESS" jdbcType="VARCHAR"/>
        <result property="isEnabled" column="IS_ENABLED" jdbcType="INTEGER"/>
        <result property="remark" column="REMARK" jdbcType="VARCHAR"/>
        <result property="workPointId" column="WORK_POINT_ID" jdbcType="INTEGER"/>
        <result property="workPointName" column="WORK_POINT_NAME" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="SupplierCol">
        SUPPLIER_ID, SUPPLIER_NAME, SUPPLIER_TYPE, CONTACT, CONTACT_NUMBER, CONTACT_ADDRESS, IS_ENABLED, REMARK, WORK_POINT_ID, WORK_POINT_NAME    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="SupplierMap">
        select
        <include refid="SupplierCol"></include>
        from EIMS.SUPPLIER
        where SUPPLIER_ID = #{supplierId}
    </select>

    <!--查询所有-->
    <select id="queryAll" resultMap="SupplierMap">
        select
        <include refid="SupplierCol"></include>
        from (select * from EIMS.SUPPLIER where work_point_id = #{workPointId})
    </select>

    <!--通过实体作为或者条件查询-->
    <select id="queryOrByPojo" resultMap="SupplierMap">
        select
        <include refid="SupplierCol"></include>
        from (select * from EIMS.SUPPLIER where work_point_id = #{workPointId})
        <where>
            <if test="supplierId != null">
                or SUPPLIER_ID like '%'||#{supplierId}||'%'
            </if>
            <if test="supplierName != null and supplierName != ''">
                or SUPPLIER_NAME like '%'||#{supplierName}||'%'
            </if>
            <if test="supplierType != null and supplierType != ''">
                or SUPPLIER_TYPE like '%'||#{supplierType}||'%'
            </if>
            <if test="contact != null and contact != ''">
                or CONTACT like '%'||#{contact}||'%'
            </if>
            <if test="contactNumber != null and contactNumber != ''">
                or CONTACT_NUMBER like '%'||#{contactNumber}||'%'
            </if>
            <if test="contactAddress != null and contactAddress != ''">
                or CONTACT_ADDRESS like '%'||#{contactAddress}||'%'
            </if>
            <if test="isEnabled != null">
                or IS_ENABLED like '%'||#{isEnabled}||'%'
            </if>
            <if test="remark != null and remark != ''">
                or REMARK like '%'||#{remark}||'%'
            </if>
            <if test="workPointName != null and workPointName != ''">
                or WORK_POINT_NAME like '%'||#{workPointName}||'%'
            </if>
        </where>
    </select>

    <!--通过实体作为并且条件查询-->
    <select id="queryAndByPojo" resultMap="SupplierMap">
        select
        <include refid="SupplierCol"></include>
        from EIMS.SUPPLIER
        <where>
            <if test="supplierId != null">
                and SUPPLIER_ID = #{supplierId}
            </if>
            <if test="supplierName != null and supplierName != ''">
                and SUPPLIER_NAME = #{supplierName}
            </if>
            <if test="supplierType != null and supplierType != ''">
                and SUPPLIER_TYPE = #{supplierType}
            </if>
            <if test="contact != null and contact != ''">
                and CONTACT = #{contact}
            </if>
            <if test="contactNumber != null and contactNumber != ''">
                and CONTACT_NUMBER = #{contactNumber}
            </if>
            <if test="contactAddress != null and contactAddress != ''">
                and CONTACT_ADDRESS = #{contactAddress}
            </if>
            <if test="isEnabled != null">
                and IS_ENABLED = #{isEnabled}
            </if>
            <if test="remark != null and remark != ''">
                and REMARK = #{remark}
            </if>
            <if test="workPointId != null">
                and WORK_POINT_ID = #{workPointId}
            </if>
            <if test="workPointName != null and workPointName != ''">
                and WORK_POINT_NAME = #{workPointName}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert">
        insert into EIMS.SUPPLIER
        <trim prefix="(" suffix=")" suffixOverrides=",">
            SUPPLIER_ID,
            <if test="supplierName != null">
                SUPPLIER_NAME,
            </if>
            <if test="supplierType != null">
                SUPPLIER_TYPE,
            </if>
            <if test="contact != null">
                CONTACT,
            </if>
            <if test="contactNumber != null">
                CONTACT_NUMBER,
            </if>
            <if test="contactAddress != null">
                CONTACT_ADDRESS,
            </if>
            <if test="isEnabled != null">
                IS_ENABLED,
            </if>
            <if test="remark != null">
                REMARK,
            </if>
            <if test="workPointId != null">
                WORK_POINT_ID,
            </if>
            <if test="workPointName != null">
                WORK_POINT_NAME,
            </if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            #{supplierId},
            <if test="supplierName != null">
                #{supplierName},
            </if>
            <if test="supplierType != null">
                #{supplierType},
            </if>
            <if test="contact != null">
                #{contact},
            </if>
            <if test="contactNumber != null">
                #{contactNumber},
            </if>
            <if test="contactAddress != null">
                #{contactAddress},
            </if>
            <if test="isEnabled != null">
                #{isEnabled},
            </if>
            <if test="remark != null">
                #{remark},
            </if>
            <if test="workPointId != null">
                #{workPointId},
            </if>
            <if test="workPointName != null">
                #{workPointName},
            </if>
        </trim>
        <selectKey keyProperty="supplierId" resultType="INTEGER" order="BEFORE">
            select SEQ_SUPPLIER.nextval from dual
        </selectKey>
    </insert>

    <!--批量新增数据 -->
    <insert id="insertBatch">
        <foreach collection="list" item="entity" open="begin" separator=";" close=";end;">
            insert into EIMS.SUPPLIER
            <trim prefix="(" suffix=")" suffixOverrides=",">
                SUPPLIER_ID,
                <if test="entity.supplierName != null">
                    SUPPLIER_NAME,
                </if>
                <if test="entity.supplierType != null">
                    SUPPLIER_TYPE,
                </if>
                <if test="entity.contact != null">
                    CONTACT,
                </if>
                <if test="entity.contactNumber != null">
                    CONTACT_NUMBER,
                </if>
                <if test="entity.contactAddress != null">
                    CONTACT_ADDRESS,
                </if>
                <if test="entity.isEnabled != null">
                    IS_ENABLED,
                </if>
                <if test="entity.remark != null">
                    REMARK,
                </if>
                <if test="entity.workPointId != null">
                    WORK_POINT_ID,
                </if>
                <if test="entity.workPointName != null">
                    WORK_POINT_NAME,
                </if>
            </trim>
            values
            <trim prefix="(" suffix=")" suffixOverrides=",">
                SEQ_SUPPLIER.NEXTVAL,
                <if test="entity.supplierName != null">
                    #{entity.supplierName},
                </if>
                <if test="entity.supplierType != null">
                    #{entity.supplierType},
                </if>
                <if test="entity.contact != null">
                    #{entity.contact},
                </if>
                <if test="entity.contactNumber != null">
                    #{entity.contactNumber},
                </if>
                <if test="entity.contactAddress != null">
                    #{entity.contactAddress},
                </if>
                <if test="entity.isEnabled != null">
                    #{entity.isEnabled},
                </if>
                <if test="entity.remark != null">
                    #{entity.remark},
                </if>
                <if test="entity.workPointId != null">
                    #{entity.workPointId},
                </if>
                <if test="entity.workPointName != null">
                    #{entity.workPointName},
                </if>
            </trim>
        </foreach>
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update EIMS.SUPPLIER
        <set>
            <if test="supplierName != null">
                SUPPLIER_NAME = #{supplierName},
            </if>
            <if test="supplierType != null">
                SUPPLIER_TYPE = #{supplierType},
            </if>
            <if test="contact != null">
                CONTACT = #{contact},
            </if>
            <if test="contactNumber != null">
                CONTACT_NUMBER = #{contactNumber},
            </if>
            <if test="contactAddress != null">
                CONTACT_ADDRESS = #{contactAddress},
            </if>
            <if test="isEnabled != null">
                IS_ENABLED = #{isEnabled},
            </if>
            <if test="remark != null">
                REMARK = #{remark},
            </if>
            <if test="workPointId != null">
                WORK_POINT_ID = #{workPointId},
            </if>
            <if test="workPointName != null">
                WORK_POINT_NAME = #{workPointName},
            </if>
        </set>
        where SUPPLIER_ID = #{supplierId}
    </update>

    <!--批量修改数据-->
    <update id="updateBatch">
        <foreach collection="list" item="entity" open="begin" separator=";" close=";end;">
            update EIMS.SUPPLIER
            <set>
                <if test="entity.supplierName != null">
                    SUPPLIER_NAME = #{entity.supplierName},
                </if>
                <if test="entity.supplierType != null">
                    SUPPLIER_TYPE = #{entity.supplierType},
                </if>
                <if test="entity.contact != null">
                    CONTACT = #{entity.contact},
                </if>
                <if test="entity.contactNumber != null">
                    CONTACT_NUMBER = #{entity.contactNumber},
                </if>
                <if test="entity.contactAddress != null">
                    CONTACT_ADDRESS = #{entity.contactAddress},
                </if>
                <if test="entity.isEnabled != null">
                    IS_ENABLED = #{entity.isEnabled},
                </if>
                <if test="entity.remark != null">
                    REMARK = #{entity.remark},
                </if>
                <if test="entity.workPointId != null">
                    WORK_POINT_ID = #{entity.workPointId},
                </if>
                <if test="entity.workPointName != null">
                    WORK_POINT_NAME = #{entity.workPointName},
                </if>
            </set>
            where SUPPLIER_ID = #{entity.supplierId}
        </foreach>
    </update>

    <!--通过主键删除数据-->
    <delete id="deleteById">
        delete from EIMS.SUPPLIER where SUPPLIER_ID = #{supplierId}
    </delete>

    <!--通过主键列表批量删除数据-->
    <delete id="deleteBatch">
        delete from EIMS.SUPPLIER where SUPPLIER_ID in
        <foreach collection="list" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <!--通过实体列表批量删除数据-->
    <delete id="deleteBatchByEntities">
        delete from EIMS.SUPPLIER where SUPPLIER_ID in
        <foreach collection="list" item="entity" open="(" separator="," close=")">
            #{entity.supplierId}
        </foreach>
    </delete>
</mapper>

